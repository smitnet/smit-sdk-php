{
    "name": "smit/smit-php-sdk",
    "description": "SMIT PHP SDK in order to communicate with our services",
    "keywords": [
        "smit",
        "sdk"
    ],
    "homepage": "https://gitlab.com/pietersmitmediabv/sdk/php.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Joey van Wijnbergen",
            "email": "support@smit.net",
            "homepage": "https://smit.net",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^5.6|^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "paragonie/random_compat": "^1|^2|^9.99"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpunit/phpunit": "^5.7|^6.0",
        "squizlabs/php_codesniffer": "^2.3|^3.0"
    },
    "autoload": {
        "psr-4": {
            "SMIT\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SMIT\\SDK\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
